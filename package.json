{
    "name": "create-solid-storybook",
    "version": "8.6.4",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "turbo run build",
        "dev": "turbo run dev",
        "types": "bun run tsc -b",
        "clean": "rm -rf .turbo && rm -rf node_modules",
        "prepublishOnly": "bun run build && bun run clean-publish",
        "clean-publish": "rm -rf cli/template/node_modules cli/template/storybook-static cli/template/.turbo cli/*.log cli/dist && echo 'ðŸ§¹ cleaned cli dir'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kachurun/create-solid-storybook.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/kachurun/create-solid-storybook/issues"
    },
    "homepage": "https://github.com/kachurun/create-solid-storybook#readme",
    "description": "SolidJS integration with Storybook",
    "packageManager": "bun@1.1.13",
    "workspaces": [
        "apps/*",
        "packages/*",
        "cli/template"
    ],
    "devDependencies": {
        "@flexbe/eslint-config": "^1.0.9",
        "@tsconfig/strictest": "^2.0.5",
        "bun": "^1.2.14",
        "eslint": "^9.27.0",
        "tsup": "^8.5.0",
        "turbo": "^2.5.3",
        "vitest": "^3.1.4"
    },
    "publishConfig": {
        "access": "public"
    },
    "bin": {
        "create-solid-storybook": "./cli/index.mjs"
    },
    "files": [
        "cli/**",
        "README.md"
    ]
}
