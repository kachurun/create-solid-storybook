{
    "name": "@kachurun/storybook-solid-vite",
    "version": "9.0.0-rc.0",
    "description": "SolidJS integration with Storybook",
    "private": false,
    "keywords": [
        "storybook",
        "solidjs",
        "solid",
        "storybook-addon",
        "storybook-integration",
        "component-library",
        "ui-components",
        "vite",
        "solid-vite",
        "solid-storybook",
        "frontend",
        "javascript",
        "typescript"
    ],
    "homepage": "https://github.com/kachurun/create-solid-storybook",
    "bugs": {
        "url": "https://github.com/kachurun/create-solid-storybook/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/kachurun/create-solid-storybook.git",
        "directory": "packages/storybook-solid-vite"
    },
    "funding": {
        "type": "opencollective",
        "url": "https://opencollective.com/storybook"
    },
    "license": "MIT",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./preset": {
            "types": "./dist/preset.d.ts",
            "require": "./dist/preset.js",
            "import": "./dist/preset.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*",
        "template/**/*",
        "README.md",
        "*.js",
        "*.d.ts"
    ],
    "scripts": {
        "prepack": "bun run build",
        "build": "tsup",
        "dev": "tsup --watch",
        "postbuild": "bun run types",
        "types": "tsc -b tsconfig.build.json"
    },
    "devDependencies": {
        "@kachurun/storybook-solid": "workspace:*",
        "@storybook/builder-vite": "next",
        "@storybook/types": "next",
        "@storybook/core": "next",
        "solid-js": "~1.9.0",
        "storybook": "next",
        "tsup-preset-solid": "^2.2.0",
        "vite": "^5.0.0 || ^6.0.0",
        "typescript": "^5.8.3"
    },
    "peerDependencies": {
        "solid-js": "~1.9.0",
        "storybook": "next",
        "vite": "^5.0.0 || ^6.0.0",
        "vite-plugin-solid": "^2.10.0"
    },
    "publishConfig": {
        "access": "public",
        "tag": "next"
    }
}
