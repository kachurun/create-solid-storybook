{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "allowJs": true,
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "types": ["vite/client"],
        "paths": {
            "#.storybook/*": ["./.storybook/*"]
        }
    },
    "include": [
        ".storybook/**/*.ts",
        ".storybook/**/*.tsx",
        "stories/**/*.ts",
        "stories/**/*.tsx",
        "stories/types/*.d.ts"
    ]
}
