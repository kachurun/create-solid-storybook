{
    "name": "sb-solid-vite",
    "version": "8.6.0",

    "description": "SolidJS integration with Storybook",
    "keywords": [
        "storybook",
        "solidjs"
    ],
    "homepage": "https://github.com/kachurun/sb-solid",
    "bugs": {
        "url": "https://github.com/kachurun/sb-solid/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/kachurun/sb-solid.git",
        "directory": "packages/framework"
    },
    "funding": {
        "type": "opencollective",
        "url": "https://opencollective.com/storybook"
    },
    "license": "MIT",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./preset": {
            "types": "./dist/preset.d.ts",
            "require": "./dist/preset.js",
            "import": "./dist/preset.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*",
        "template/**/*",
        "README.md",
        "*.js",
        "*.d.ts"
    ],
    "scripts": {
        "prepack": "bun run build --optimized --reset",
        "build": "tsup src/index.ts src/preset.ts --format esm,cjs --out-dir dist",
        "postbuild": "bun run types",
        "types": "tsc -b"
    },
    "dependencies": {
        "@storybook/builder-vite": "next",
        "@storybook/types": "next",
        "sb-solid": "workspace:*"
    },
    "devDependencies": {
        "solid-js": "~1.9.0",
        "storybook": "^8.6.0",
        "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
    },
    "peerDependencies": {
        "solid-js": "~1.9.0",
        "storybook": "^8.6.0",
        "vite": "^4.0.0 || ^5.0.0 || ^6.0.0",
        "vite-plugin-solid": "^2.10.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
