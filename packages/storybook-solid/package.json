{
    "name": "@kachurun/storybook-solid",
    "version": "9.0.0-next.2",
    "private": false,
    "description": "Storybook SolidJS renderer",
    "keywords": [
        "storybook",
        "solidjs",
        "solid",
        "storybook-addon",
        "storybook-integration",
        "component-library",
        "ui-components",
        "vite",
        "solid-vite",
        "solid-storybook",
        "frontend",
        "javascript",
        "typescript"
    ],
    "homepage": "https://github.com/kachurun/create-solid-storybook",
    "bugs": {
        "url": "https://github.com/kachurun/create-solid-storybook/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/kachurun/create-solid-storybook.git",
        "directory": "packages/storybook-solid"
    },
    "funding": {
        "type": "opencollective",
        "url": "https://opencollective.com/storybook"
    },
    "license": "MIT",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./preset": "./preset.js",
        "./dist/entry-preview.mjs": "./dist/entry-preview.mjs",
        "./dist/entry-preview-docs.mjs": "./dist/entry-preview-docs.mjs",
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*",
        "template/**/*",
        "README.md",
        "*.js",
        "*.d.ts"
    ],
    "scripts": {
        "prepack": "bun run build",
        "build": "tsup",
        "dev": "tsup --watch",
        "postbuild": "bun run types",
        "types": "tsc -b tsconfig.build.json"
    },
    "devDependencies": {
        "@babel/standalone": "^7.26.2",
        "@types/babel__standalone": "^7.1.9",
        "storybook": "next",
        "@storybook/docs-tools": "next",
        "@storybook/core": "next",
        "@storybook/global": "^5.0.0",
        "@storybook/preview-api": "next",
        "@storybook/types": "next",
        "async-mutex": "^0.5.0",
        "esbuild-plugin-solid": "^0.6.0",
        "type-fest": "^4.41.0",
        "typescript": "^5.8.3"
    },
    "peerDependencies": {
        "solid-js": "~1.9.0",
        "@storybook/test": "next",
        "storybook": "next"
    },
    "peerDependenciesMeta": {
        "@storybook/test": {
            "optional": true
        }
    },
    "publishConfig": {
        "access": "public",
        "tag": "next"
    }
}
